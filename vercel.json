{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/front/dist/index.html"
    }
  ],
  "outputDirectory": "front/dist"
}
